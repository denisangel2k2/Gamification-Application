            <Font name="Franklin Gothic Demi" size="26.0" />
         </font>
      </Label>
      <TextField fx:id="numberTextField" layoutX="94.0" layoutY="197.0" prefHeight="25.0" prefWidth="64.0" />
      <Label fx:id="timeLabel" layoutX="56.0" layoutY="234.0" />
      <Label fx:id="tokensObtainedLabel" layoutX="56.0" layoutY="264.0" />
      <Button fx:id="guessButton" layoutX="168.0" layoutY="197.0" mnemonicParsing="false" onAction="#guessButtonAction" text="Guess" />
      <TextArea fx:id="descriptionTextArea" editable="false" layoutX="56.0" layoutY="75.0" prefHeight="90.0" prefWidth="200.0" wrapText="true" />
      <Button fx:id="backButton" layoutX="258.0" layoutY="361.0" mnemonicParsing="false" onAction="#backButtonAction" text="Back" />
